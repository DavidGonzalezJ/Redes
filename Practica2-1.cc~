#include <sys/types.h>
#include <unistd.h>
#include <stdio.h>
#include <string.h>
#include <iostream>
#include <errno.h>

extern int errno;

int main() {
   /* Comprobar la ocurrencia de error y notificarlo con la llamada perror(3) */
if(setuid(0) == -1){
	perror("Error al hacer setuid ");
	printf( "Error Value is : %s\n", strerror(errno));
	printf("%i\n", errno);
    }
    return 1;
}
